<div class="container">
	<%= form_tag(search_page_path, :method => "get",   
	    class: 'navbar-form navbar-left') do %>  
	  <div class="input-group mb-3">  
	    <%= search_field_tag :search, params[:search], placeholder: "Enter a sourch query", class: "form-control" %>  
	    <div class="input-group-btn">  
	      <%= button_tag "Search", :class => 'btn btn-outline-primary',:name => nil%>  
	    </div>  
	  </div>  
	<% end %>
</div>	